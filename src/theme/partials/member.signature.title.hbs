{{#compact}}
  {{#if parent.flags.isPrivate}}
    private&nbsp;
  {{/if}}
  {{#if parent.flags.isProtected}}
    protected&nbsp;
  {{/if}}
  {{#ifCond kindString '==' 'Call signature'}}
    function&nbsp;
  {{/ifCond}}
  {{#unless hideName}}
    {{{name}}}
  {{/unless}}
  {{#if typeParameters}}
    {{#each typeParameters}}
      {{#if @index}}, {{/if}}
      {{{name}}}
    {{/each}}
  {{/if}}
  (
  {{#each parameters}}
    {{#if @index}},&nbsp;{{/if}}
    {{#if flags.isRest}}...{{/if}}
    {{{name}}}
    {{#if flags.isOptional}}?{{/if}}
    {{#if defaultValue}}?{{/if}}
    :&nbsp;
    {{{type}}}
  {{/each}}
  )
  {{#conditional type '!=' 'void'}}
    :&nbsp;
    {{{type}}}
  {{/conditional}}
{{/compact}}
